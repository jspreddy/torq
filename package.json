{
  "name": "@jspreddy/midas",
  "version": "0.1.16-0",
  "description": "my dynamo library",
  "source": "src/index.ts",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "types": "dist/types.d.ts",
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "test:all": "npm run ddb-local:cleanup && jest",
    "test:watch": "npm run ddb-local:cleanup && jest --watch",
    "ddb-local:cleanup": "npx kill-port 8000",
    "test": "jest /tests/unit-tests/",
    "test:int": "npm run ddb-local:cleanup && jest /tests/integration-tests/",
    "test:int:watch": "npm run ddb-local:cleanup && jest --watch /tests/integration-tests/",
    "test:unit": "jest /tests/unit-tests/",
    "test:unit:watch": "jest --watch /tests/unit-tests/",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint --fix .",
    "watch": "parcel watch",
    "build": "npm run clean && parcel build",
    "clean": "rm -rf ./dist && rm -rf ./.parcel-cache"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.678.0",
    "@aws-sdk/lib-dynamodb": "^3.678.0",
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.25.0",
    "@babel/preset-typescript": "^7.24.7",
    "@eslint/js": "^9.8.0",
    "@parcel/packager-ts": "^2.12.0",
    "@parcel/transformer-typescript-types": "^2.12.0",
    "@shelf/jest-dynamodb": "^3.5.0",
    "@types/eslint__js": "^8.42.3",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.7.7",
    "babel-jest": "^29.7.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "kill-port": "^2.0.1",
    "parcel": "^2.12.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^7.17.0"
  },
  "dependencies": {
    "eslint-plugin-jest": "^28.6.0",
    "lodash": "^4.17.21"
  }
}
